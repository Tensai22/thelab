@using Microsoft.AspNetCore.Mvc.Localization
@inject IViewLocalizer Localizer
<section class="banner-home-2">
	<div class="w-layout-blockcontainer base-container w-container">
		<div class="w-layout-hflex home-2-banner-top">
			<div class="w-layout-vflex about-title">
				<div class="w-layout-vflex vertical-20">
					<div class="position-relative">
						<h1>@Localizer["Creating Stories That Inspire"]</h1>
					</div>
					<div class="banner-2-description">
						<div class="paragraph-large gray-color">@Localizer["At TheLab, we bring your vision to life through captivating storytelling and cinematic excellence."]</div>
					</div>
				</div>
			</div>
			<a href="#Scroll-link" class="scroll-link mobile-hidden w-inline-block">
				<div>@Localizer["Scroll for more"]</div>
				<div class="dot"></div>
			</a>
		</div>
		<a href="#" id="Scroll-link" class="lightbox-home-2 w-inline-block w-lightbox">
			<div class="relative-20">
				<div class="w-layout-hflex primary-button-white">
					<div>@Localizer["Watch reel"]</div>
					<div class="clip">
						<div class="clip-icon-wrapper">
							<img src="~/images/group-201-20-1-.svg" loading="lazy" alt="icon" class="btn-icon" />
						</div>
						<div class="clip-icon-wrapper right">
							<img src="~/images/group-201-20-4-.svg" loading="lazy" alt="icon" class="btn-icon" />
						</div>
					</div>
				</div>
			</div>
			<div class="full-video-bg z1 w-background-video w-background-video-atom">
				<video id="video-bg" autoplay loop muted playsinline style='background-image:url("~/images/daa0-4d63-bb94-0c2046acf5d8-poster-00001.jpg")' data-object-fit="cover">
					<source src="~/images/daa0-4d63-bb94-0c2046acf5d8-transcode.mp4" type="video/mp4" />
					<source src="~/images/daa0-4d63-bb94-0c2046acf5d8-transcode.webm" type="video/webm" />
				</video>
			</div>
			<script type="application/json" class="w-json">
				{
				  "items": [
					{
					  "url": "https://www.youtube.com/watch?v=Q84Ji3PQWLM",
					  "originalUrl": "https://www.youtube.com/watch?v=Q84Ji3PQWLM",
					  "width": 940,
					  "height": 528,
					  "thumbnailUrl": "https://i.ytimg.com/vi/Q84Ji3PQWLM/hqdefault.jpg",
					  "html": "<iframe class=\"embedly-embed\" src=\"//cdn.embedly.com/widgets/media.html?src=https%3A%2F%2Fwww.youtube.com%2Fembed%2FQ84Ji3PQWLM%3Ffeature%3Doembed&display_name=YouTube&url=https%3A%2F%2Fwww.youtube.com%2Fwatch%3Fv%3DQ84Ji3PQWLM&image=https%3A%2F%2Fi.ytimg.com%2Fvi%2FQ84Ji3PQWLM%2Fhqdefault.jpg&type=text%2Fhtml&schema=youtube\" width=\"940\" height=\"528\" scrolling=\"no\" title=\"YouTube embed\" frameborder=\"0\" allow=\"autoplay; fullscreen; encrypted-media; picture-in-picture;\" allowfullscreen=\"true\"></iframe>",
					  "type": "video"
					}
				  ],
				  "group": ""
				}
			</script>
		</a>
	</div>
</section>
<section class="section">
	<div class="w-layout-blockcontainer base-container w-container">
		<div class="w-layout-vflex vertical-100">
			<div class="w-layout-hflex subtitle">
				<div>@Localizer["About Studio"]</div>
				<div class="dot"></div>
			</div>
			<div class="w-layout-hflex about-home-wrapper">
				<div class="about-title-wrapper">
					<h2>@Localizer["A premier video"] <span class="text-span-image">.</span> production studio where creativity and innovation come together to craft visually stunning and impactful content.</h2>
					<div class="about-small-video w-background-video w-background-video-atom">
						<video autoplay loop muted playsinline style='background-image:url("~/images/small-poster-00001.jpg")' data-object-fit="cover">
							<source src="~/images/small-transcode.mp4" type="video/mp4" />
							<source src="~/images/small-transcode.webm" type="video/webm" />
						</video>
					</div>
				</div>
				<div class="w-layout-vflex vertical-40 max-w">
					<p class="paragraph-large paragraph-no-margin">@Localizer["With a focus"]</p>
				</div>
			</div>
			<div class="w-layout-grid numbers-grid">
				<div class="w-layout-vflex">
					<div class="w-layout-vflex vertical-8">
						<div class="number">500+</div>
						<div class="upper-text">Projects Completed</div>
					</div>
					<p class="margin-top-20">Successful video productions for clients across various industries.</p>
				</div>
				<div class="w-layout-vflex">
					<div class="w-layout-vflex vertical-8">
						<div class="number">100+</div>
						<div class="upper-text">Happy Clients</div>
					</div>
					<p class="margin-top-20">Trusted by top brands and businesses worldwide.</p>
				</div>
				<div class="w-layout-vflex">
					<div class="w-layout-vflex vertical-8">
						<div class="number">2,000+</div>
						<div class="upper-text">Hours of Footage</div>
					</div>
					<p class="margin-top-20">Capturing every angle, every moment, and every emotion.</p>
				</div>
				<div class="w-layout-vflex">
					<div class="w-layout-vflex vertical-8">
						<div class="number">20+</div>
						<div class="upper-text">Award-Winning Videos</div>
					</div>
					<p class="margin-top-20">Recognized for outstanding creativity and production quality.</p>
				</div>
			</div>
		</div>
	</div>
</section>
<section class="section without-top-spacing">
	<div class="w-layout-blockcontainer base-container w-container">
		<div class="w-layout-grid process-grid">
			<div class="w-layout-vflex vertical-40">
				<h2>We believe that every great video starts with a clear vision.</h2>
				<div class="w-layout-vflex vertical-16">
					<div class="w-layout-hflex horizontal-20">
						<div class="upper-text-600 gray-color">01.</div>
						<div class="upper-text-600">Discovery &amp; Planning</div>
					</div>
					<div class="proces-item-description">
						<div class="gray-color">We start by getting to know your brand, your vision, and your target audience.</div>
					</div>
				</div>
				<div class="w-layout-vflex vertical-16">
					<div class="w-layout-hflex horizontal-20">
						<div class="upper-text-600 gray-color">02.</div>
						<div class="upper-text-600">Concept &amp; Scripting</div>
					</div>
					<div class="proces-item-description">
						<div class="gray-color">Our team of experts collaborates with you to develop a strong concept and compelling script.</div>
					</div>
				</div>
				<div class="w-layout-vflex vertical-16">
					<div class="w-layout-hflex horizontal-20">
						<div class="upper-text-600 gray-color">03.</div>
						<div class="upper-text-600">Pre-Production</div>
					</div>
					<div class="proces-item-description">
						<div class="gray-color">From location scouting and casting to scheduling and organizing, we handle all the logistics to ensure a smooth and efficient production phase.</div>
					</div>
				</div>
				<div class="w-layout-vflex vertical-16">
					<div class="w-layout-hflex horizontal-20">
						<div class="upper-text-600 gray-color">04.</div>
						<div class="upper-text-600">Production</div>
					</div>
					<div class="proces-item-description">
						<div class="gray-color">Our skilled crew works tirelessly to capture every moment with precision, using the latest equipment and techniques to achieve the highest quality results.</div>
					</div>
				</div>
				<div class="w-layout-vflex vertical-16">
					<div class="w-layout-hflex horizontal-20">
						<div class="upper-text-600 gray-color">05.</div>
						<div class="upper-text-600">Delivery &amp; Review</div>
					</div>
					<div class="proces-item-description">
						<div class="gray-color">We work closely with you to make sure everything is just right, making any necessary revisions before final delivery.</div>
					</div>
				</div>
			</div>
			<div class="our-process-video-wrapper">
				<div class="absolute-video w-background-video w-background-video-atom">
					<video autoplay loop muted playsinline style='background-image:url("~/images/yellow-poster-00001.jpg")' data-object-fit="cover">
						<source src="~/images/yellow-transcode.mp4" type="video/mp4" />
						<source src="~/images/yellow-transcode.webm" type="video/webm" />
					</video>
				</div>
				<div class="w-layout-hflex subtitle">
					<div>Our Process</div>
					<div class="dot"></div>
				</div>
			</div>
		</div>
	</div>
</section>
<section class="section without-top-spacing">
	<div class="w-layout-blockcontainer base-container w-container">
		<div class="background-video w-background-video w-background-video-atom">
			<video id="background-video"
				   autoplay=""
				   loop=""
				   style='background-image:url("~/images/dfe3-445a-bc6a-6de7501d210b-poster-00001.jpg")'
				   muted=""
				   playsinline=""
				   data-object-fit="cover">
				<source src="~/images/dfe3-445a-bc6a-6de7501d210b-transcode.mp4" type="video/mp4" />
				<source src="~/images/dfe3-445a-bc6a-6de7501d210b-transcode.webm" type="video/webm" />
			</video>
			<div class="subtitle-top">
				<div class="w-layout-hflex subtitle">
					<div>Our Values</div>
					<div class="dot"></div>
				</div>
			</div>
			<div class="values-title">
				<h2 class="text-white">Our values are at the core of everything we do.</h2>
			</div>
			<div class="w-layout-hflex values-items-wrapper">
				<div class="first-values-item">
					<div class="w-layout-vflex values-item">
						<div class="w-layout-hflex values-item-title">
							<div>Creativity</div>
							<div class="dot white"></div>
						</div>
						<div>We are passionate about bringing innovative ideas to life.</div>
					</div>
				</div>
				<div class="w-layout-grid right-values-items">
					<div class="w-layout-vflex values-item">
						<div class="w-layout-hflex values-item-title">
							<div>Collaboration</div>
							<div class="dot white"></div>
						</div>
						<div>We believe in the power of teamwork.</div>
					</div>
					<div class="w-layout-vflex values-item">
						<div class="w-layout-hflex values-item-title">
							<div>Quality</div>
							<div class="dot white"></div>
						</div>
						<div>We are committed to delivering the highest-quality videos that showcase the best of your brand.</div>
					</div>
					<div class="w-layout-vflex values-item">
						<div class="w-layout-hflex values-item-title">
							<div>Integrity</div>
							<div class="dot white"></div>
						</div>
						<div>We value transparency and honesty in every aspect of our work.</div>
					</div>
				</div>
			</div>
		</div>
	</div>
</section>
<section class="section overflow-hidden-dark">
	<div class="w-layout-blockcontainer base-container position-relative w-container">
		<div class="w-layout-vflex vertical-40">
			<div class="w-layout-hflex subtitle">
				<div>the team</div>
				<div class="dot"></div>
			</div>
			<div class="w-layout-vflex vertical-20">
				<h2 class="quote-title text-white">The Lab was founded by Eric Labbonski.</h2>
				<div class="quote-description">
					<div class="gray-color paragraph-large">
						A visionary in the world of video production with a deep passion for storytelling and creative excellence.
					</div>
				</div>
			</div>
			<div class="quote-video z-1 w-background-video w-background-video-atom">
				<video autoplay loop muted playsinline style='background-image:url("~/images/523b-4675-8716-1b62195303ef-poster-00001.jpg")' data-object-fit="cover">
					<source src="~/images/523b-4675-8716-1b62195303ef-transcode.mp4" type="video/mp4" />
					<source src="~/images/523b-4675-8716-1b62195303ef-transcode.webm" type="video/webm" />
				</video>
			</div>
			<div class="w-layout-vflex quote-wrapper light">
				<div class="quote-text-wrapper">
					<h3>
						At The Lab, we believe that every frame tells a story. Our mission is to turn ideas into unforgettable experiences, where creativity and technology unite to inspire and engage.
					</h3>
				</div>
				<div class="w-layout-vflex vertical-8">
					<h6 class="upper-text-600">Eric Labbonski</h6>
					<h6 class="upper-text gray-color">CEO of thelab</h6>
				</div>
			</div>
		</div>
		<div class="quote-absolute light"></div>
	</div>
</section>